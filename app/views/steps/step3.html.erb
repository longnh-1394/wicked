<%= form_with(model: @user, method: :put, url: wizard_path, local: true) do |form| %>
  <div class="field">
    <%= form.label :address %>
    <%= form.text_field :address, id: :user_address %>
  </div>

  <div class="field">
    <%= form.label :phone %>
    <%= form.text_field :phone, id: :user_phone %>
  </div>

  <div class="actions">
    <%= form.submit %>
    <%= link_to "back", previous_wizard_path %>
  </div>
<% end %>
