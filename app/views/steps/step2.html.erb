<%= form_with(model: @user, method: :put, url: wizard_path, local: true) do |form| %>
  <div class="field">
    <%= form.label :first_name %>
    <%= form.text_field :first_name, id: :user_first_name %>
  </div>

  <div class="field">
    <%= form.label :last_name %>
    <%= form.text_field :last_name, id: :user_last_name %>
  </div>

  <div class="actions">
    <%= form.submit "Next Step" %>
    <%= link_to "back", previous_wizard_path %>
  </div>
<% end %>
